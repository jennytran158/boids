# - Config file for the FooBar package
# It defines the following variables
#  boids_INCLUDE_DIRS - include directories for project boids
#  boids_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(PROJECT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

## Our library dependencies (contains definitions for IMPORTED targets)
include("${PROJECT_CMAKE_DIR}/@PROJECT_NAME@-targets.cmake")

set(boids_INCLUDE_DIRS "@PROJECT_INCLUDE_DIRS@")
set(boids_LIBRARIES "@PROJECT_LIBS@")
